<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <PaddleInferenceDlls>$(MSBuildThisFileDirectory)..\..\runtimes</PaddleInferenceDlls>
  </PropertyGroup>
  <ItemGroup Condition="$(TargetFrameworkVersion.StartsWith('v4')) Or $(TargetFramework.StartsWith('net4'))">
    <Content Include="$(PaddleInferenceDlls)\win-x64\native\libiomp5md.dll">
      <Link>dll\x64\libiomp5md.dll</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="$(PaddleInferenceDlls)\win-x64\native\mkldnn.dll">
      <Link>dll\x64\mkldnn.dll</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="$(PaddleInferenceDlls)\win-x64\native\mklml.dll">
      <Link>dll\x64\mklml.dll</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="$(PaddleInferenceDlls)\win-x64\native\paddle_inference_c.dll">
      <Link>dll\x64\paddle_inference_c.dll</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>
</Project>